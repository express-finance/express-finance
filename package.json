{
	"name": "express-finance",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "yarn env && next dev",
		"build": "yarn prisma generate && next build",
		"start": "next start",
		"lint": "next lint",
		"env": "doppler secrets download --format env --no-file --project express-finance --config dev > .env",
		"prisma:dev": "yarn prisma migrate dev",
		"prisma:deploy": "yarn prisma migrate deploy",
		"prisma:generate": "yarn prisma generate"
	},
	"dependencies": {
		"@auth0/nextjs-auth0": "^1.9.1",
		"@headlessui/react": "^1.6.6",
		"@heroicons/react": "^1.0.6",
		"@prisma/client": "^4.2.1",
		"axios": "^0.27.2",
		"formik": "^2.2.9",
		"next": "12.2.5",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-hot-toast": "^2.3.0"
	},
	"devDependencies": {
		"@types/node": "18.7.6",
		"@types/react": "18.0.17",
		"@types/react-dom": "18.0.6",
		"autoprefixer": "^10.4.8",
		"eslint": "8.22.0",
		"eslint-config-next": "12.2.5",
		"postcss": "^8.4.16",
		"prisma": "^4.2.1",
		"tailwindcss": "^3.1.8",
		"typescript": "4.7.4"
